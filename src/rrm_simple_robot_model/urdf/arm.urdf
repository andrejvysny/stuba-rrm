<?xml version="1.0"?>
<robot name="arm">

  <!--Links-->
  <link name="base_link">
    <visual>
      <geometry>
        <cylinder length="0.01" radius="0.1"/>
      </geometry>
      <material name="white">
        <color rgba="1 1 1 0.4"/>
      </material>
    </visual>
  </link>

  <link name="link_1">
    <visual>
      <geometry>
       <sphere radius="0.03"/>
      </geometry>
       <material name="grey">
            <color rgba="0.2 0.2 0.2 0.4"/>
          </material>
        <origin xyz="0 0 0" rpy = "0 0 0"/>
    </visual>
  </link>

  <link name="link_2">
     <visual>
      <geometry>
        <cylinder length="0.178" radius = "0.01"/>
      </geometry>
      <material name="white">
           <color rgba="1 1 1 0.4"/>
         </material>
      <origin xyz="0 0 0.089" rpy = "0 0 0"/>
    </visual>
  </link>

  <link name="link_3">
    <visual>
      <geometry>
        <cylinder length="0.203" radius = "0.01"/>
      </geometry>
         <material name="white">
              <color rgba="1 1 1 0.4"/>
            </material>
      <origin xyz="0 0 0.1015" rpy = "0 0 0"/>
    </visual>
  </link>

  <link name="link_4">
    <visual>
      <geometry>
        <cylinder length="0.05" radius = "0.01"/>
      </geometry>
        <material name="white">
          <color rgba="1 1 1 0.4"/>
        </material>
      <origin xyz="0 0 0.025" rpy = "0 0 0"/>
    </visual>
  </link>

    <link name="link_5">
      <visual>
        <geometry>
          <cylinder length="0.15" radius = "0.01"/>
        </geometry>
            <material name="white">
              <color rgba="1 1 1 0.4"/>
            </material>
        <origin xyz="0 0 0.075" rpy = "0 0 0"/>
      </visual>
    </link>

    <link name="link_6">
      <visual>
        <geometry>
          <cylinder length="0.1" radius = "0.01"/>
        </geometry>
          <material name="blue">
            <color rgba="0 1 1 0.4"/>
          </material>
        <origin xyz="0 0 0.05" rpy = "0 0 0"/>
      </visual>
    </link>



 <link name="tool0">
    <visual>
       <geometry>
          <box size="0.03 0.03 0.06" />
       </geometry>
       <material name="black">
         <color rgba="0 0 0 0.1"/>
       </material>
       <origin xyz="0 0 0.03" rpy="0 0 0" />
     </visual>
 </link>



  <!--Joints-->
  <joint name="joint_1" type="revolute">
    <axis xyz="0 0 1"/>
    <parent link="base_link"/>
    <child link="link_1"/>
    <origin xyz="0 0 0" rpy = "0 0 0"/>
    <limit effort="1000.0" lower="-1.62" upper="1.62" velocity="0.5"/>
  </joint>

  <joint name="joint_2" type="revolute">
    <axis xyz="0 1 0"/>
    <parent link="link_1"/>
    <child link="link_2"/>
    <origin xyz="0 0 0" rpy = "0 0 0"/>
    <limit effort="1000.0" lower="-0.96" upper="2.182" velocity="0.5"/>
  </joint>

    <joint name="joint_3" type="revolute">
      <axis xyz="0 1 0"/>
      <parent link="link_2"/>
      <child link="link_3"/>
      <origin xyz="0 0 0.178" rpy = "0 0 0"/>
      <limit effort="1000.0" lower="-0.96" upper="2.182" velocity="0.5"/>
    </joint>


    <joint name="joint_4" type="revolute">
      <axis xyz="0 0 1"/>
      <parent link="link_3"/>
      <child link="link_4"/>
      <origin xyz="0 0 0.203" rpy = "0 0 0"/>
      <limit effort="1000.0" lower="-3.14" upper="3.14" velocity="0.5"/>
    </joint>

    <joint name="joint_5" type="revolute">
      <axis xyz="0 1 0"/>
      <parent link="link_4"/>
      <child link="link_5"/>
      <origin xyz="0 0 0.05" rpy = "0 0 0"/>
      <limit effort="1000.0" lower="-2.2" upper="2.2" velocity="0.5"/>
    </joint>


    <joint name="joint_6" type="prismatic">
      <axis xyz="0 1 0"/>
      <parent link="link_5"/>
      <child link="link_6"/>
      <origin xyz="0 0 0.15" rpy = "0 0 0"/>
      <limit effort="1000.0" lower="0" upper="0.1" velocity="0.5"/>
    </joint>



    <joint name="joint_3-tool0" type="fixed">
      <parent link="link_6"/>
      <child link="tool0"/>
      <origin xyz="0 0 0.1" rpy = "0 0 0"/>
    </joint>


</robot>